{
    "debugEnabled": true,

    /*
    Print modes:
        JSON    = 0
        Line    = 1
        Pretty  = 2
    */
    "printMode": 2,

    /*
    Aliases:
        List persistent aliases here. commands matching "name"
        will instead execute the value of "command"
        Use $# for interpolation
    */
    "aliases": [
        {
            "name": ".quit",
            "command": ".exit"
        },
        {
            "name": ".all",
            "command": ".query select * from $#"
        }
    ]
}