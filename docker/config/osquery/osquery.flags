--verbose
--tls_dump
--tls_hostname=goserver:8001
--config_plugin=tls
--distributed_plugin=tls
--enroll_tls_endpoint=/enroll
--config_tls_endpoint=/config
--enroll_secret_path=/tmp/secret
--allow_unsafe
--db_path=/tmp/osqueryd
